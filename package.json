{
  "name": "freepo-prompt",
  "version": "1.0.0",
  "description": "TypeScript Electron Application",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "tsc -p tsconfig.main.json && tsc -p tsconfig.renderer.json && electron .",
    "build": "tsc -p tsconfig.main.json && tsc -p tsconfig.renderer.json",
    "watch": "tsc -p tsconfig.main.json -w & tsc -p tsconfig.renderer.json -w"
  },
  "dependencies": {
    "electron": "^28.0.0",
    "ignore": "^7.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.19.111",
    "typescript": "^5.0.0"
  }
}
